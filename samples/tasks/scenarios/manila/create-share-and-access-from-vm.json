{
    "version": 2,
    "title": "Manila Create Share And Access From Vm",
    "description": "Test creating Manila share resources",
    "subtasks": [
        {
            "title": "Create share and access from vm",
            "scenario": {
                "ManilaShares.create_share_and_access_from_vm": {
                    "size": 1,
                    "share_type": "CephNFStype",
                    "flavor": {
                        "name": "m1.tiny"
                    },
                    "image": {
                        "name": "^cirros.*-disk$"
                    },
                    "username": "ubuntu"
                }
            },
            "runner": {
                "constant": {
                    "times": 2,
                    "concurrency": 2
                }
            },
            "contexts": {
                "quotas": {
                    "manila": {
                        "shares": -1,
                        "gigabytes": -1
                    }
                },
                "users": {
                    "tenants": 2,
                    "users_per_tenant": 1
                },
                "network": {
                    "dns_nameservers": [
                        "9.9.9.9"
                    ]
                }
            },
            "sla": {
                "failure_rate": {
                    "max": 0
                }
            }
        }
    ]
}
