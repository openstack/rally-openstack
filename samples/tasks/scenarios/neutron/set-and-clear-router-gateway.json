{
    "version": 2,
    "title": "Neutron Set And Clear Router Gateway",
    "description": "Test set and clear router gateway operations",
    "subtasks": [
        {
            "title": "Set and clear router gateway",
            "scenario": {
                "NeutronNetworks.set_and_clear_router_gateway": {
                    "network_create_args": {
                        "router:external": true
                    },
                    "router_create_args": {}
                }
            },
            "runner": {
                "constant": {
                    "times": 4,
                    "concurrency": 2
                }
            },
            "contexts": {
                "network": {},
                "users": {
                    "tenants": 2,
                    "users_per_tenant": 2
                },
                "quotas": {
                    "neutron": {
                        "network": -1,
                        "router": -1
                    }
                },
                "roles": [
                    "admin"
                ]
            },
            "sla": {
                "failure_rate": {
                    "max": 0
                }
            }
        }
    ]
}
