{
    "version": 2,
    "title": "Cinder Volume Creation from Snapshot",
    "description": "Test scenario for creating volumes from snapshots in Cinder block storage service.",
    "subtasks": [
        {
            "title": "Create Volume From Snapshot",
            "scenario": {
                "CinderVolumes.create_volume_from_snapshot": {
                    "do_delete": true
                }
            },
            "runner": {
                "constant": {
                    "times": 3,
                    "concurrency": 2
                }
            },
            "contexts": {
                "users": {
                    "tenants": 2,
                    "users_per_tenant": 2
                },
                "volumes": {
                    "size": 1
                }
            },
            "sla": {
                "failure_rate": {
                    "max": 0
                }
            }
        }
    ]
}
