{% set flavor_name = flavor_name or "m1.tiny" %}
{
    "version": 2,
    "title": "Unknown Dd Load Test",
    "description": "Test dd load test operations",
    "subtasks": [
        {
            "title": "Unknown dd load test",
            "scenario": {
                "VMTasks.dd_load_test": {
                    "flavor": {
                        "name": "{{flavor_name}}"
                    },
                    "image": {
                        "name": "^cirros.*-disk$"
                    },
                    "floating_network": "public",
                    "force_delete": false,
                    "interpreter": "/bin/sh",
                    "username": "cirros"
                }
            },
            "runner": {
                "constant": {
                    "times": 10,
                    "concurrency": 2
                }
            },
            "contexts": {
                "users": {
                    "tenants": 3,
                    "users_per_tenant": 2
                },
                "network": {}
            },
            "sla": {
                "failure_rate": {
                    "max": 0
                }
            }
        }
    ]
}
